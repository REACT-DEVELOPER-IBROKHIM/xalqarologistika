{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "@components/*": ["./src/components/*"],
            "@selectors/*": ["./src/redux/selectors/*"],
            "@thunks/*": ["./src/redux/thunks/*"],
            "@slices/*": ["./src/redux/slices/*"],
            "@builders/*": ["./src/redux/builders/*"],
